<Window x:Class="ServerChat.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ServerChat"
        mc:Ignorable="d"
        Title="SERVER" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="Notify:"></TextBlock>
            <TextBlock x:Name="lbNotify" Foreground="Green" FontSize="30"></TextBlock>
            <Button Name="btStartServer" Content="Start Server" Click="btStartServer_Click"></Button>
            
        </StackPanel>
    </Grid>
</Window>
